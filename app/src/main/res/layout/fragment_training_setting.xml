<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/button_save"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Save"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.9" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="616dp" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/text_field_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Title"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.google.android.material.textfield.TextInputEditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="text" />

    </com.google.android.material.textfield.TextInputLayout>

    <ImageButton
        android:id="@+id/button_warmup_minus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_baseline_remove_24"
        tools:layout_editor_absoluteX="281dp"
        tools:layout_editor_absoluteY="411dp" />

    <TextView
        android:id="@+id/text_view_warmup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:lines="2"
        android:maxLines="2"
        android:text="Warmup 00:00d"
        tools:layout_editor_absoluteX="84dp"
        tools:layout_editor_absoluteY="423dp" />

    <ImageButton
        android:id="@+id/button_warmup_plus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_baseline_add_24"
        tools:layout_editor_absoluteX="96dp"
        tools:layout_editor_absoluteY="263dp" />

    <ImageButton
        android:id="@+id/button_workout_minus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_baseline_remove_24"
        tools:layout_editor_absoluteX="230dp"
        tools:layout_editor_absoluteY="220dp" />

    <TextView
        android:id="@+id/text_view_workout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Workout 00:00"
        tools:layout_editor_absoluteX="263dp"
        tools:layout_editor_absoluteY="306dp" />

    <ImageButton
        android:id="@+id/button_workout_plus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_baseline_add_24"
        tools:layout_editor_absoluteX="182dp"
        tools:layout_editor_absoluteY="263dp" />

    <ImageButton
        android:id="@+id/button_exercice_rest_minus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_baseline_remove_24"
        tools:layout_editor_absoluteX="182dp"
        tools:layout_editor_absoluteY="325dp" />

    <TextView
        android:id="@+id/text_view_exercice_rest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Exercise rest 00:00"
        tools:layout_editor_absoluteX="57dp"
        tools:layout_editor_absoluteY="306dp" />

    <ImageButton
        android:id="@+id/button_exercice_rest_plus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_baseline_add_24"
        tools:layout_editor_absoluteX="182dp"
        tools:layout_editor_absoluteY="220dp" />

    <ImageButton
        android:id="@+id/button_set_rest_minus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_baseline_remove_24"
        tools:layout_editor_absoluteX="134dp"
        tools:layout_editor_absoluteY="220dp" />

    <TextView
        android:id="@+id/text_view_set_rest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Set rest 00:00"
        tools:layout_editor_absoluteX="176dp"
        tools:layout_editor_absoluteY="306dp" />

    <ImageButton
        android:id="@+id/button_set_rest_plus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_baseline_add_24"
        tools:layout_editor_absoluteX="96dp"
        tools:layout_editor_absoluteY="325dp" />

    <ImageButton
        android:id="@+id/button_round_minus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_baseline_remove_24"
        tools:layout_editor_absoluteX="190dp"
        tools:layout_editor_absoluteY="368dp" />

    <TextView
        android:id="@+id/text_view_round_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Round 0"
        tools:layout_editor_absoluteX="202dp"
        tools:layout_editor_absoluteY="423dp" />

    <ImageButton
        android:id="@+id/button_round_plus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_baseline_add_24"
        tools:layout_editor_absoluteX="269dp"
        tools:layout_editor_absoluteY="263dp" />

    <ImageButton
        android:id="@+id/button_set_minus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_baseline_remove_24"
        tools:layout_editor_absoluteX="269dp"
        tools:layout_editor_absoluteY="325dp" />

    <TextView
        android:id="@+id/text_view_set_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Set 0"
        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="380dp" />

    <ImageButton
        android:id="@+id/button_set_plus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_baseline_add_24"
        tools:layout_editor_absoluteX="99dp"
        tools:layout_editor_absoluteY="368dp" />

    <androidx.constraintlayout.helper.widget.Flow
        android:id="@+id/flow2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:constraint_referenced_ids="button_warmup_minus,text_view_warmup,button_warmup_plus,button_workout_minus,text_view_workout,button_workout_plus,button_exercice_rest_minus,text_view_exercice_rest,button_exercice_rest_plus,button_set_rest_minus,text_view_set_rest,button_set_rest_plus,button_round_minus,text_view_round_number,button_round_plus,button_set_minus,text_view_set_number,button_set_plus"
        app:flow_firstHorizontalStyle="packed"
        app:flow_firstVerticalStyle="packed"
        app:flow_maxElementsWrap="3"
        app:flow_wrapMode="aligned"
        app:layout_constraintBottom_toTopOf="@+id/button_save"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_field_title"
        app:layout_constraintVertical_bias="1.0" />


</androidx.constraintlayout.widget.ConstraintLayout>